{
    "name": "inani/larapoll",
    "description": "A Laravel package to create polls",
    "keywords": ["polls", "votes", "laravel", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "Inani El Houssain",
            "email": "inanielhoussain@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {
        "Inani\\Larapoll\\": "src/"
      }
    },
    "require": {
      "illuminate/support": "~5|~6|~7|~8|~9|~10|^12.0" 
        
    },
   
    "require-dev": {
      "fakerphp/faker": "^1.9.1",
      "phpunit/phpunit": "^10.0"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Inani\\Larapoll\\LarapollServiceProvider"
        ],
        "aliases": {
          "PollWriter": "Inani\\Larapoll\\PollWriterFacade"
        }
      }
    }
}
